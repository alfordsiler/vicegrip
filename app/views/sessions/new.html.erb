<div class="container">
  <h1>Login</h1>
  <br>
  <%= link_to "<< Home", root_path %><br><br>

  <%= form_for :user do |f| %>
  <%= f.email_field :email, placeholder: "Enter your email" %>
  <%= f.password_field :password, placeholder: "Enter your password" %>
  <button type="submit" class="btn navbar-btn btn-toolbar">Login</button>
  <% end %>
</div>